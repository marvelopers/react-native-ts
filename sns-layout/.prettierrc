{
  "bracketSpacing": true,
  "jsxBracketSameLine": true,
  "printWidth": 100,
  "semi": true,
  "tabWidth": 2,
  "trailingComma": "all",
  "useTabs": false,
  "singleQuote": true,
  "proseWrap": "always",
  "arrowParens": "avoid",
  "overrides": [
    {
      "files": ["**/*.ts", "**/*.tsx"],
      "options": {
        "parser": "typescript"
      }
    }
  ]
}
